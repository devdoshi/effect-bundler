{
  "unstable": ["sloppy-imports"],
  "compilerOptions": {
    "noImplicitAny": false,
    "jsxImportSource": "solid",
    "jsx": "precompile"
  },
  "tasks": {
    "dev": "deno run --watch main.ts"
  },
  "fmt": {
    "semiColons": false
  },
  "workspace": [
    "./lib/solid-router"
  ],
  "imports": {
    "effect": "npm:effect@^3.10.19",
    "effect/platform": "npm:@effect/platform@^0.69.30",
    "solid": "npm:solid-js@^1.9.3",
    "solid/jsx-runtime": "./solid-jsx-runtime.ts",
    "solid/router/": "@nounder/solid-router/",
    "tailwindcss": "npm:tailwindcss@^3.4.15",
    "vite": "npm:vite@^6.0.1"
  }
}
